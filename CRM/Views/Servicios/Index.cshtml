@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Dashboard";
}

<div class="pt-16 lg:pl-64 pl-0 bg-white dark:bg-gray-900">
    <div class="p-4 pt-40 sm:ml-64">
        <div class="p-1">
            <nav class="flex" aria-label="Breadcrumb">
                <ol class="inline-flex items-center space-x-1 md:space-x-2 rtl:space-x-reverse">
                    @HtmlElement.TailwindBreadCrumb(Html,new [] {"Index","Servicios"})
                </ol>
            </nav>
            <br />
            <div class="p-1">
                <h4 class="text-2xl font-bold dark:text-white">Administrar Servicios</h4>
                <div class="mt-3 grid grid-cols-12 mb-3">
                    <div class="flex items-center justify-between flex-column flex-wrap md:flex-row space-y-4 md:space-y-0 pb-4 bg-white dark:bg-gray-900">
                        <label for="table-search" class="sr-only">Search</label>
                        <div class="relative">
                            <div class="mt-4 grid grid-cols-12 md:grid-cols-1 gap-4 mx-auto">
                                @HtmlElement.Input(Html,1,"Prueba","txtInput","Esto es una prueba","","","")
                            </div>
                        </div>
                    </div>
                    <div class="relative overflow-x-auto shadow-md sm:rounded-lg">
                      

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

